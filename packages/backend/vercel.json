{
  "version": 2,
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["../shared/dist/**"]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.ts" }]
}
